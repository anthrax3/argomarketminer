{
   "compilerOptions": {
      "declaration": false,
      "experimentalDecorators": true,
      "emitDecoratorMetadata": true,
      "isolatedModules": false,
      "jsx": "react",
      "lib": [
         "es6",
         "dom"
      ],
      "module": "commonjs",
      "moduleResolution": "node",
      "noImplicitAny": false,
      "noUnusedParameters": true,
      "noUnusedLocals": true,
      "noLib": false,
      "preserveConstEnums": true,
      "removeComments": true,
      "sourceMap": true,
      "suppressImplicitAnyIndexErrors": true,
      "strictNullChecks": true,
      "target": "ES6"
   }
}