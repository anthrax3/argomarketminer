{
    "compilerOptions": {
        "declaration": false,
        "experimentalDecorators": true,
        "emitDecoratorMetadata": true,
        "isolatedModules": false,
        "jsx": "react",
        "moduleResolution": "node",
        "noImplicitAny": false,
        "noUnusedParameters": false,
        "noUnusedLocals": true,
        "noLib": false,
        "preserveConstEnums": true,
        "removeComments": true,
        "sourceMap": true,
        "suppressImplicitAnyIndexErrors": true
    }
}